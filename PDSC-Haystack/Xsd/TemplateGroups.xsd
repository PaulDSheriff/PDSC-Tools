              <xs:element name="TemplateFileName" type="xs:string" />
              <xs:element name="IsSelected" type="xs:string" />
              <xs:element name="IsVisible" type="xs:string" />
              <xs:element name="IsRequired" type="xs:string" />
              <xs:element name="ProjectId" type="xs:string" />
              <xs:element name="IsPDSAFramework" type="xs:string" />
              <xs:element name="AddDataMember" type="xs:string" />
              <xs:element name="UseDataAnnotations" type="xs:string" />
